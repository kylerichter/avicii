{
  "name": "avicii",
  "version": "1.0.0",
  "description": "Avicii Discord music bot",
  "main": "src/bot/index.ts",
  "repository": "https://github.com/kylerichter/avicii.git",
  "author": "kingpluto",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && tsc && mkdir dist/files",
    "start": "node dist/bot/index.js",
    "deploy-commands": "node dist/deploy-commands.js"
  },
  "dependencies": {
    "@discordjs/voice": "0.17.0",
    "config": "3.3.11",
    "discord.js": "14.15.2",
    "ffmpeg-static": "5.2.0",
    "googleapis": "137.1.0",
    "libsodium-wrappers": "0.7.13",
    "youtube-dl-exec": "3.0.2"
  },
  "devDependencies": {
    "@types/config": "3.3.4",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "9.2.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">= 21 <22"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "semi": 0,
      "comma-dangle": 0,
      "prefer-const": "error"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "useTabs": false
  }
}
